algoritmo "conteAte10"
var
      contador, valor : inteiro
inicio
      contador <- 0
      Escreval ("Quer contar até quanto? ")
      leia (valor)
      enquanto (contador <= valor) faca
      escreval (contador)
               contador <- contador + 1
      fimEnquanto
      
      Escreval ("Terminei de contar")
      

fimalgoritmo