algoritmo "conteAte10"
var
      contador : inteiro
inicio
      contador <- 0
      enquanto (contador <= 10) faca
      escreval (contador)
               contador <- contador + 1
      fimEnquanto
      
      Escreval ("Terminei de contar, cheguei até ", contador, " pq comecei do zero")
      

      

fimalgoritmo