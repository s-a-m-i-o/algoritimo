algoritmo "Soma"
var
    numeroUm: real
    numeroDois: real
    soma: real
inicio

      escreval ("teste de calculo")
      escreval ("Digite um número ")
      leia (numeroUm)
      escreval ("Digite outro número")
      leia (numeroDois)
      soma <- numeroUm+numeroDois
      escreval ("A soma de ", numeroUm, " + ", numerodois, " = ", soma )

fimalgoritmo