algoritmo "Emprestimo"
var
   emprestimo, aumento, parcela, valor_emprestimo:real
inicio
      Escreval ("Qual valor do imprestimo?")
      leia(emprestimo)
      Escreval ("Em quantas parcelas? ")
      leia(parcela)
      valor_emprestimo <- emprestimo/parcela
      Escreval ("Fica em ",valor_emprestimo, " parcelas")

      

fimalgoritmo